[
    Test 1
    Poprawne zwijanie staÅ‚ych
    
    Przykladowe dane testowe:
        1. ? 0
           > 40
           > 40
        2. ? 3
           > 40
           > 0
]
DECLARE
	a, b, tab(0:1000)
BEGIN
    READ a;
    b := 0;
    b := 0/b;
    FOR _ FROM 0 TO b DO
        tab(a) := 40 + b;
    ENDFOR
    WRITE tab(a);
    WRITE tab(b);
END
