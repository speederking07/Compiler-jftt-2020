[
    Test 11
    Algorytm euklidesa

    Przykladowe dane testowe:
        1. ? 312
           ? 424
           > 8
        2. ? 3123
           ? 2442342
           > 3
        3. ? 12345
           ? 54321
           > 3
]
DECLARE
    a, b, c
BEGIN
    READ a;
    READ b;
    WHILE b != 0 DO
        c := a % b;
        a := b;
        b := c;
    ENDWHILE
    WRITE a;
END