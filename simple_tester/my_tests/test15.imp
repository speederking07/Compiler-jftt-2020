    Test 15
    StaÅ‚a Eulera

    Przykladowe dane testowe:
        1. ? 3
           > 271
        2. ? 20
           > 27182818284590452353
]
DECLARE
    e, n, fact, c, tmp
BEGIN
    READ n;
    n := n + 2;
    c := 1;
    FOR _ FROM 1 TO n DO
        c := c * 10;
    ENDFOR
    e := 0;    
    FOR k FROM 0 TO n DO
        fact := 1;
        FOR j FROM 1 TO k DO
            fact := fact * j;
        ENDFOR
        tmp := c / fact;
        e := e + tmp;
    ENDFOR
    e := e / 1000;
    WRITE e;
END