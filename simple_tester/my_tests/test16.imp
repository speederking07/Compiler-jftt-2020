DECLARE
    x, y, n, even
BEGIN
    READ x;
    READ n;
    IF n = 0 THEN
        WRITE 1;
    ELSE
        y := 1;   
        WHILE n > 1 DO
            even := n % 2;
            IF even = 0 THEN 
                x := x * x;
                n := n / 2;
            ELSE
                y := x * y;
                x := x * x;
                n := n - 1;
                n := n / 2;
            ENDIF
        ENDWHILE
        x := x * y;
        WRITE x;
    ENDIF
END