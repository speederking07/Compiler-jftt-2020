[
    Test 26
    PÄ™tla Until dobrze eliminowana

    Przykladowe dane testowe:
        1.  ? 2
            > 4
            > 64
]
DECLARE
    a, b
BEGIN
    READ a;
    b := 4;
    REPEAT
        a := a + a;
    UNTIL b = 4;
    WRITE a;
    REPEAT
        a := a * 2;
        b := b - 1;
    UNTIL b = 0;
    WRITE a;
END