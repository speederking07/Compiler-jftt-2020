[
    Test 7
    ZarzÄ…dzanie rejestrami

    Przykladowe dane testowe:
        ? 4
        ? 2
        > 6
        > 13
]
DECLARE
	a, b, c, d, e, f, g, h, i, j
BEGIN
    READ a;
    READ b;
    c := a + b;
    d := a - b;
    e := a / b;
    f := a % b;
    g := c;
    h := g;
    i := h + d;
    j := i + e;
    FOR iter FROM 0 TO e DO
        j := j + iter;
        g := g + 1;
        h := e / e;
        g := g - h;
    ENDFOR
    WRITE g;
    WRITE j;
END