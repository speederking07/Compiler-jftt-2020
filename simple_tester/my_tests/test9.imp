DECLARE
	j, k, tab(0:10000)
BEGIN
    tab(0) := 0;
    tab(1) := 1;
    FOR i FROM 2 TO 10000 DO
        j := i - 1;
        k := i - 2;
        tab(i) := tab(j) + tab(k);
    ENDFOR
    READ k;
    WRITE tab(k);
END